<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @log.title %>
</p>

<p>
  <%= markdown(@log.entry).html_safe, class: 'entry' %>
</p>

<p>
  <%= image_tag @log.image.url, class: 'img-sm right' %>
</p>

<%= link_to 'Edit', edit_log_path(@log), class: 'btn btn-primary' %> |
<%= link_to 'Back', logs_path, class: 'btn btn-primary' %>
