<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @log.title %>
</p>

<p>
  <strong>Entry:</strong>
  <%= markdown(@log.entry).html_safe %>
</p>

<p>
  <strong>Image:</strong>
  <%= image_tag @log.image.url, class: 'img-sm right' %>
</p>

<%= link_to 'Edit', edit_log_path(@log) %> |
<%= link_to 'Back', logs_path %>
